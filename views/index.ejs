        outline: none;
      }

      .rs-select2 .select2-container .select2-selection--single {
        outline: none;
        border: none;
        height: 34px;
      }

      .rs-select2
        .select2-container
        .select2-selection--single
        .select2-selection__rendered {
        line-height: 34px;
        padding-left: 0;
        color: #555;
      }

      .rs-select2
        .select2-container
        .select2-selection--single
        .select2-selection__arrow {
