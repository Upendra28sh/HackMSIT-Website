        -ms-flex-pack: center;
        justify-content: center;
        -webkit-box-align: center;
        -webkit-align-items: center;
        -moz-box-align: center;
        -ms-flex-align: center;
        align-items: center;
      }

      .rs-select2
        .select2-container
        .select2-selection--single
        .select2-selection__arrow
